            function: idlab-fn:random
            parameters: []
    po:
      - [a, sm:OnlineContactPoint]
      - [a, sm:ContactPoint]
      - p: sm:hasEmail
        o:
          mapping: email
      - p: sm:hasTelephone
        o:
          mapping: telephone
      - p: sm:hasWebSite
        o:
          mapping: website
      - p: sm:isOnlineContactPointOf
        o:
          mapping: poi
