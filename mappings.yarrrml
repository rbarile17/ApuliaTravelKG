prefixes:
 ex: http://example.com/
 grel: http://users.ugent.be/~bjdmeest/function/grel.ttl#
 idlab-fn: https://w3id.org/imec/idlab/function#
 clv: https://w3id.org/italia/onto/CLV/
 sm: https://w3id.org/italia/onto/SM/
 access-condition: https://w3id.org/italia/onto/AccessCondition/
 ti: https://w3id.org/italia/onto/TI/
 poi: https://w3id.org/italia/onto/POI/
 l0: https://w3id.org/italia/onto/l0/
 td: https://example.org/td/
mappings:
  geometry:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: clv:Geometry/$(id_attrattore)
    po:
      - [a, clv:Geometry]
      - [clv:lat, $(latitudine)]
      - [clv:coordinate, $(latitudine)]
      - [clv:long, $(longitudine)]
      - [clv:coordinate, $(longitudine)]
      - p: sm:isGeometryFor
        o:
          mapping: poi
  rating:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: sm:Rating/$(cod_ranking)
    po:
      - [a, sm:Rating]
      - p: sm:userRating
        o:
          value: $(cod_ranking)
          datatype: xsd:int
  review:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s:
      function: grel:array_join
      parameters:
        - [grel:p_array_a, sm:Review/]
        - parameter: grel:p_array_a
          value:
            function: idlab-fn:random
            parameters: []
    po:
      - [a, sm:Review]
      - p: sm:hasRating
        o:
          mapping: rating
  website:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: sm:WebSite/$(web)
    po:
      - [a, sm:WebSite]
      - p: sm:isWebSiteOf
        o:
          mapping: contactPoint
  telephone:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: sm:Telephone/$(tel_transf)
    po:
      - [a, sm:Telephone]
      - [sm:telephoneNumber, $(tel_transf)]
      - p: sm:isTelephoneOf
        o:
          mapping: contactPoint
  email:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: sm:Email/$(email)
    po:
      - [a, sm:Email]
      - p: sm:emailAddress
        o:
          value: $(email)
          datatype: xsd:anyURI
      - p: sm:isEmailOf
        o:
          mapping: contactPoint
  addressArea:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: clv:AddressArea/$(ambito_territoriale)
    po:
      - [a, clv:AddressArea]
      - [l0:name, $(ambito_territoriale)]
  streetToponym:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s:
      function: grel:array_join
      parameters:
        - [grel:p_array_a, clv:StreetToponym/]
        - parameter: grel:p_array_a
          value:
            function: idlab-fn:random
            parameters: []
    po:
      - [a, clv:StreetToponym]
      - [rdfs:label, $(indirizzo)]
  city:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: clv:City/$(comune)
    po:
      - [a, clv:City]
      - [l0:name, $(comune)]
      - p: clv:hasHigherRank
        o:
          mapping: province
      - p: clv:situatedWithin
        o:
          mapping: province
  province:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: clv:Province/$(provincia)
    po:
      - [a, clv:Province]
      - [clv:acronym, $(sigla_provincia)]
      - [l0:name, $(provincia)]
      - p: clv:hasLowerRank
        o:
          mapping: city
  streetNumber:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s:
      function: grel:array_join
      parameters:
        - [grel:p_array_a, clv:StreetNumber/]
        - parameter: grel:p_array_a
          value:
            function: idlab-fn:random
            parameters: []
    po:
      - [a, clv:CivicNumbering]
      - p: clv:streetNumber
        o:
          value: $(civico)
          datatype: xsd:string
  address:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s:
      function: grel:array_join
      parameters:
        - [grel:p_array_a, clv:Address/]
        - parameter: grel:p_array_a
          value:
            function: idlab-fn:random
            parameters: []
    po:
      - [a, clv:Address]
      - p: clv:hasAddressComponent
        o:
          mapping: city
      - p: clv:hasCity
        o:
          mapping: city
      - p: clv:hasAddressComponent
        o:
          mapping: province
      - p: clv:hasProvince
        o:
          mapping: province
      - p: clv:hasAddressComponent
        o:
          mapping: streetToponym
      - p: clv:hasStreetToponym
        o:
          mapping: streetToponym
      - p: clv:hasAddressComponent
        o:
          mapping: addressArea
      - p: clv:hasAddressArea
        o:
          mapping: addressArea
      - p: clv:isAddressFor
        o:
          mapping: poi
      - p: clv:hasAddressComponent
        o:
          mapping: streetNumber
      - p: clv:hasNumber
        o:
          mapping: streetNumber
      - [clv:postCode, $(cap)]
  openingHoursMorning:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: clv:OpeningHours/poi_$(id_attrattore)_opening_hours_morning
    condition:
      function: grel:boolean_not
      parameters:
        - parameter: grel:bool_b
          value:
            function: idlab-fn:isNull
            parameters:
              - [idlab-fn:str, $(orario_apertura_mattina)]
    po:
      - [a, clv:OpeningHoursSpecification]
      - p: clv:opens
        o:
          function: grel:string_substring
          parameters:
            - [grel:valueParameter, $(orario_apertura_mattina)]
            - [grel:param_int_i_from, 0]
            - [grel:param_int_i_opt_to, 5]
      - p: clv:closes
        o:
          function: grel:string_substring
          parameters:
            - [grel:valueParameter, $(orario_apertura_mattina)]
            - [grel:param_int_i_from, 8]
            - [grel:param_int_i_opt_to, 13]
      - p: access-condition:isAccessConditionOf
        o:
          mapping: poi
      - p: ti:hasDayOfWeek
        o:
          value: ti:Monday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(LUNEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Tuesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MARTEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Wednesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MERCOLEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o:
          value: ti:Thursday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(GIOVEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Friday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(VENERDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Saturday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(SABATO)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Sunday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(DOMENICA)]
            - [grel:valueParameter2, "true"]
  openingHoursAfternoon:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: clv:OpeningHours/poi_$(id_attrattore)_opening_hours_afternoon
    condition:
      function: grel:boolean_not
      parameters:
        - parameter: grel:bool_b
          value:
            function: idlab-fn:isNull
            parameters:
              - [idlab-fn:str, $(orario_apertura_pomeriggio)]
    po:
      - [a, clv:OpeningHoursSpecification]
      - p: clv:opens
        o:
          function: grel:string_substring
          parameters:
            - [grel:valueParameter, $(orario_apertura_pomeriggio)]
            - [grel:param_int_i_from, 0]
            - [grel:param_int_i_opt_to, 5]
      - p: clv:closes
        o:
          function: grel:string_substring
          parameters:
            - [grel:valueParameter, $(orario_apertura_pomeriggio)]
            - [grel:param_int_i_from, 8]
            - [grel:param_int_i_opt_to, 13]
      - p: access-condition:isAccessConditionOf
        o:
          mapping: poi
      - p: ti:hasDayOfWeek
        o:
          value: ti:Monday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(LUNEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Tuesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MARTEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Wednesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MERCOLEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o:
          value: ti:Thursday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(GIOVEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Friday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(VENERDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Saturday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(SABATO)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Sunday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(DOMENICA)]
            - [grel:valueParameter2, "true"]
  contactPoint:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s:
      function: grel:array_join
      parameters:
        - [grel:p_array_a, sm:ContactPoint/]
        - parameter: grel:p_array_a
