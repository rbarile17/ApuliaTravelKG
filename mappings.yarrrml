  poi:
    sources:
      - [attrattori_small.json~jsonpath, "$.[*]"]
    s: poi:PointOfInterest/$(id_attrattore)
    po:
      - [a, poi:PointOfInterest]
      - [a, td:TouristDestination]
      - p: a
        o: [td:NaturalPark]
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(tipologia)]
            - [grel:valueParameter2, "PARCHI_NATURALI"]
      - p: a
        o: [td:NationalPark]
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(tipologia)]
            - [grel:valueParameter2, "PARCHI_NAZIONALI"]
      - p: a
