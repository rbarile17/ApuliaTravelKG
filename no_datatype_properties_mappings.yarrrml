prefixes:
 ex: http://example.com/
 grel: http://users.ugent.be/~bjdmeest/function/grel.ttl#
 idlab-fn: https://w3id.org/imec/idlab/function#
 clv: https://w3id.org/italia/onto/CLV/
 sm: https://w3id.org/italia/onto/SM/
 access-condition: https://w3id.org/italia/onto/AccessCondition/
 ti: https://w3id.org/italia/onto/TI/
 poi: https://w3id.org/italia/onto/POI/
 l0: https://w3id.org/italia/onto/l0/
 td:  https://apuliatravel.org/td/
mappings:
  geometry:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s:
      function: grel:string_replace
      parameters:
        - [grel:valueParameter, clv:Geometry/$(id_attrattore)_$(nm_attrattore_it)_geometry]
        - [grel:p_string_find, " "]
        - [grel:p_string_replace, "_"]
    po:
      - [a, clv:Geometry]
      - p: sm:isGeometryFor
        o:
          mapping: poi
  rating:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s: sm:Rating/$(cod_ranking)
    po:
      - [a, sm:Rating]
  review:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s:
      function: grel:string_replace
      parameters:
        - [grel:valueParameter, sm:Review/$(id_attrattore)_$(nm_attrattore_it)_generic_review]
        - [grel:p_string_find, " "]
        - [grel:p_string_replace, "_"]
    po:
      - [a, sm:Review]
      - p: sm:hasRating
        o:
          mapping: rating
  website:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s: sm:WebSite/$(web)
    po:
      - [a, sm:WebSite]
      - p: sm:isWebSiteOf
        o:
          mapping: contactPoint
  telephone:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s: sm:Telephone/$(tel_transf)
    po:
      - [a, sm:Telephone]
      - p: sm:isTelephoneOf
        o:
          mapping: contactPoint
  email:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s: sm:Email/$(email)
    po:
      - [a, sm:Email]
      - p: sm:isEmailOf
        o:
          mapping: contactPoint
  addressArea:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s: clv:AddressArea/$(ambito_territoriale)
    po:
      - [a, clv:AddressArea]
  streetToponym:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s:
      function: grel:string_replace
      parameters:
        - [grel:valueParameter, clv:StreetToponym/$(id_attrattore)_$(nm_attrattore_it)_street_toponym]
        - [grel:p_string_find, " "]
        - [grel:p_string_replace, "_"]
    po:
      - [a, clv:StreetToponym]
  city:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s: clv:City/$(comune)
    po:
      - [a, clv:City]
      - p: clv:hasHigherRank
        o:
          mapping: province
      - p: clv:situatedWithin
        o:
          mapping: province
  province:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s: clv:Province/$(provincia)_province
    po:
      - [a, clv:Province]
      - p: clv:hasLowerRank
        o:
          mapping: city
  streetNumber:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s:
      function: grel:string_replace
      parameters:
        - [grel:valueParameter, clv:StreetNumber/$(id_attrattore)_$(nm_attrattore_it)_street_number]
        - [grel:p_string_find, " "]
        - [grel:p_string_replace, "_"]
    po:
      - [a, clv:CivicNumbering]
  address:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s:
      function: grel:string_replace
      parameters:
        - [grel:valueParameter, clv:Address/$(id_attrattore)_$(nm_attrattore_it)_address]
        - [grel:p_string_find, " "]
        - [grel:p_string_replace, "_"]
    po:
      - [a, clv:Address]
      - p: clv:hasAddressComponent
        o:
          mapping: city
      - p: clv:hasCity
        o:
          mapping: city
      - p: clv:hasAddressComponent
        o:
          mapping: province
      - p: clv:hasProvince
        o:
          mapping: province
      - p: clv:hasAddressComponent
        o:
          mapping: streetToponym
      - p: clv:hasStreetToponym
        o:
          mapping: streetToponym
      - p: clv:hasAddressComponent
        o:
          mapping: addressArea
      - p: clv:hasAddressArea
        o:
          mapping: addressArea
      - p: clv:isAddressFor
        o:
          mapping: poi
      - p: clv:hasAddressComponent
        o:
          mapping: streetNumber
      - p: clv:hasNumber
        o:
          mapping: streetNumber
  openingHoursMorning:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s:
      function: grel:string_replace
      parameters:
        - [grel:valueParameter, clv:OpeningHours/$(id_attrattore)_$(nm_attrattore_it)_opening_hours_morning]
        - [grel:p_string_find, " "]
        - [grel:p_string_replace, "_"]
    condition:
      function: grel:boolean_not
      parameters:
        - parameter: grel:bool_b
          value:
            function: idlab-fn:isNull
            parameters:
              - [idlab-fn:str, $(orario_apertura_mattina)]
    po:
      - [a, clv:OpeningHoursSpecification]
      - p: access-condition:isAccessConditionOf
        o:
          mapping: poi
      - p: ti:hasDayOfWeek
        o:
          value: ti:Monday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(LUNEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Tuesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MARTEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Wednesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MERCOLEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o:
          value: ti:Thursday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(GIOVEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Friday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(VENERDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Saturday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(SABATO)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Sunday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(DOMENICA)]
            - [grel:valueParameter2, "true"]
  openingHoursAfternoon:
    sources:
      - [attrattori.json~jsonpath, "$.[*]"]
    s:
      function: grel:string_replace
      parameters:
        - [grel:valueParameter, clv:OpeningHours/$(id_attrattore)_$(nm_attrattore_it)_opening_hours_afternoon]
        - [grel:p_string_find, " "]
        - [grel:p_string_replace, "_"]
    condition:
      function: grel:boolean_not
      parameters:
        - parameter: grel:bool_b
          value:
            function: idlab-fn:isNull
            parameters:
              - [idlab-fn:str, $(orario_apertura_pomeriggio)]
    po:
      - [a, clv:OpeningHoursSpecification]
      - p: access-condition:isAccessConditionOf
        o:
          mapping: poi
      - p: ti:hasDayOfWeek
        o:
          value: ti:Monday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(LUNEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Tuesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MARTEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Wednesday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(MERCOLEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o:
          value: ti:Thursday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(GIOVEDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Friday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(VENERDI)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Saturday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(SABATO)]
            - [grel:valueParameter2, "true"]
      - p: ti:hasDayOfWeek
        o: 
          value: ti:Sunday
          type: iri
        condition:
          function: idlab-fn:equal
          parameters:
            - [grel:valueParameter, $(DOMENICA)]
            - [grel:valueParameter2, "true"]
